package lab22Wip;

public class ThreadRunner extends Thread
{
	//instance variables
	private String name;
	private int likelihoodRunRest;
	private int distance;
	
	public Threadrunner (String name, int likehihoodRunRuest, int distance)
	{
		this.name = name;
		this.likelihoodRunRest = likelihoodRunRest;
		this.distance = distance;
	}
		
		
	
}
	

}
